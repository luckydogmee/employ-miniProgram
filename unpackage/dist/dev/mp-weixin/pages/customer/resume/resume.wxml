<view class="resume-container"><view class="tab-container"><view data-event-opts="{{[['tap',[['switchTab',['all']]]]]}}" class="{{['tab',(type==='all')?'active':'']}}" bindtap="__e">全部简历</view><view data-event-opts="{{[['tap',[['switchTab',['started']]]]]}}" class="{{['tab',(type==='started')?'active':'']}}" bindtap="__e">已推荐过</view><view data-event-opts="{{[['tap',[['switchTab',['notStart']]]]]}}" class="{{['tab',(type==='notStart')?'active':'']}}" bindtap="__e">未推荐过</view></view><search bind:search="__e" vue-id="1" data-event-opts="{{[['^search',[['search']]]]}}" bind:__l="__l"></search><view class="list-container"><block wx:for="{{resumeList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><list-resume vue-id="{{'2-'+__i0__}}" data="{{item}}" data-event-opts="{{[['^show',[['show',['$0','$1'],['status','id']]]],['^showDetail',[['showDetail',['$0'],[[['resumeList','id',item.id,'id']]]]]],['^showDeliveryDetail',[['showDeliveryDetail',['$0'],['id']]]],['^recommend',[['recommend',['$0'],['id']]]]]}}" bind:show="__e" bind:showDetail="__e" bind:showDeliveryDetail="__e" bind:recommend="__e" bind:__l="__l"></list-resume></block></view><view data-event-opts="{{[['tap',[['showAddResume',['$event']]]]]}}" class="resume-add" bindtap="__e"><image src="../../../static/icon/add.png" mode></image></view><uni-popup class="vue-ref" vue-id="3" custom="true" showMask="{{false}}" maskClick="{{false}}" data-ref="recommendRecord" bind:__l="__l" vue-slots="{{['default']}}"><view class="recommendDialog"><view class="recommend-title">推荐记录</view><view class="recommend-table"><view class="tr"><view class="th t1">推荐时间</view><view class="th t2">岗位</view><view class="th t3">公司</view><view class="th t4">当前状态</view></view><block wx:for="{{recommendRecord}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><view class="tr"><view class="td t1">{{item.time}}</view><view class="td t2">{{item.post}}</view><view class="td t3">{{item.company}}</view><view class="td t4">{{item.status}}</view></view></block></view><image class="close-icon" src="../../../static/icon/close.png" data-event-opts="{{[['tap',[['closeRecommendDialog',['$event']]]]]}}" bindtap="__e"></image></view></uni-popup><dialog vue-id="4" showDialog="{{showDialog}}" bind:__l="__l"></dialog></view>